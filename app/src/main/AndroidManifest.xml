<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.wilczak.freedivecomp.android">

    <application
        android:allowBackup="true"
        android:name=".ui.application.FreediveApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".ui.splash.SplashActivity" android:theme="@style/AppThemeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.startinglanes.StartingLanesActivity" />
        <activity android:name=".ui.startinglist.StartingListActivity" />
        <activity android:name=".ui.pairing.PairingActivity" />
        <activity android:name=".ui.findrace.FindRaceActivity" />
        <activity android:name=".ui.enterresult.EnterResultActivity" />

    </application>
</manifest>
